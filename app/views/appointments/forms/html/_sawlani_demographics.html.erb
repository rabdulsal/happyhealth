<!-- *************************************************

	HTML FOR PDF JQERY POPUP IN 'NEW APPOINTMENTS' - CLASS NAME AND STYLESHEET LINK TAG NAMES MUST MATCH FORM NAME!!

	**************************************************** -->

<%= stylesheet_link_tag "sawlani_demographics" %>

<div class="sawlani_demographics" title="">

	<div id="container">
		<!-- <span class="form-button">
			<%#= link_to "Edit Health Form", edit_user_form_path(current_user.id, current_user.form.id), class: "submit-button on-pdf" %>
		</span> -->
		<%= render 'appointments/forms/html/pdf_form_buttons' %>

		<div class="pos line-one last-name row-one">
			<p>
				<%= user.form.personal.last_name %>
			</p>
		</div>
		<div class="pos line-one first-name">
			<p>
				<%= user.form.personal.first_name %>
			</p>
		</div>
		<div class="pos line-one middle-init">
			<p>
				<%= user.form.personal.middle_initial %>
			</p>
		</div>
		<div class="pos line-two row-one">
			<p>
				<%= user.form.personal.date_of_birth %>
			</p>
		</div>
		<div class="pos line-two gender">
			<p>
				<%= user.form.personal.gender %>
			</p>
		</div>
		<div class="pos line-three user-address row-one">
			<p>
				<%= user.form.personal.address %>
			</p>
		</div>
		<div class="pos line-two user-email">
			<p>
				<%= user.form.personal.email_address %>
			</p>
		</div>
		<div class="pos line-four row-one">
			<p>
				<%= user.form.personal.city %>
			</p>
		</div>
		<div class="pos line-four state">
			<p>
				<%= user.form.personal.state %>
			</p>
		</div>
		<div class="pos line-four zip">
			<p>
				<%= user.form.personal.zip_code %>
			</p>
		</div>
		<div class="pos line-three phone">
			<p>
				<%= user.form.personal.home_phone %>
			</p>
		</div>
		<div class="pos line-twelve employer-name">
			<p>
				<%= user.form.personal.employer %>
			</p>
		</div>
		<div class="pos line-twelve employer-address">
			<p>
				<%= user.form.personal.work_address %>
			</p>
		</div>
		<div class="pos line-four employer-phone">
			<p>
				<%= user.form.personal.employer_phone %>
			</p>
		</div>
		<div class="pos line-twelve employer-city">
			<p>
				<%= user.form.personal.employer_city %>
			</p>
		</div>
		<div class="pos line-twelve employer-state">
			<p>
				<%= user.form.personal.employer_state %>
			</p>
		</div>
		<div class="pos line-twelve employer-zip">
			<p>
				<%= user.form.personal.employer_zipcode %>
			</p>
		</div>
		<div class="pos line-ten insurance-company">
			<p>
				<%= user.form.insurances[0].company %>
			</p>
		</div>
		<div class="pos line-ten row-one">
			<p>
				<%= user.form.insurances[0].subscribers_first_name %>
				<%= user.form.insurances[0].subscribers_last_name %>
			</p>
		</div>
		<div class="pos line-eleven row-one">
			<p>
				<%= user.form.insurances[0].policy_number %>
			</p>
		</div>
		<div class="pos line-eleven insurance-group-number">
			<p>
				<%= user.form.insurances[0].group_number %>
			</p>
		</div>
		<div class="pos line-fourteen row-one">
			<p>
				<%= user.form.emergencies[1].name %>
			</p>
		</div>
		<div class="pos line-fourteen emergency-relationship">
			<p>
				<%= user.form.emergencies[1].relationship_to_patient %>
			</p>
		</div>
		<div class="pos line-fourteen emergency-phone">
			<p>
				<%= user.form.emergencies[1].phone_number %>
			</p>
		</div>
	</div>

	<div id="container_two">
		<%= render 'appointments/forms/html/pdf_form_buttons' %>

	</div>

	<div id="container_three">
		<%= render 'appointments/forms/html/pdf_form_buttons' %>

	</div>

	<div id="container_four">
		<%= render 'appointments/forms/html/pdf_form_buttons' %>

	</div>

	<div id="container_five">
		<%= render 'appointments/forms/html/pdf_form_buttons' %>

	</div>

	<div id="container_six">
		<%= render 'appointments/forms/html/pdf_form_buttons' %>

	</div>
</div>

