<% form.insurances.each do |insurance| %>

     <fieldset class="demoBackground">
        <legend><%= "#{insurance.title} Insurance" %></legend>

        <table class="show-table">

            <tr>
                    <th class="right-align">Relationship To Patient: </th>
                    <td class="left-align"> <%= insurance.relationship_to_patient %></td>
            </tr>
            <tr>
                    <th class="right-align">Insurer: </th>
                    <td class="left-align"> <%= insurance.payer %></td>
            </tr>
            <tr>
                    <th class="right-align">Insurance Address: </th>
                    <td class="left-align"><%= insurance.address %></td>
            </tr>
            <tr>
                    <th class="right-align">City: </th>
                    <td class="left-align"><%= insurance.city %></td>
            </tr>
