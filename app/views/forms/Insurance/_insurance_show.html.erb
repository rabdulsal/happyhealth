                    <td class="left-align"><%= insurance.subscribers_address %></td>
            </tr>
            <tr>
                    <th class="right-align">City: </th>
                    <td class="left-align"><%= insurance.subscribers_city %></td>
            </tr>
            <tr>
                    <th class="right-align">State: </th>
                    <td class="left-align"> <%= insurance.subscribers_state %></td>
            </tr>
            <tr>
                    <th class="right-align">Zip: </th>
                    <td class="left-align"><%= insurance.subscribers_zipcode %></td>
            </tr>
            <tr>
                    <th class="right-align">Social Security #: </th>
                    <td class="left-align"><%= insurance.social_security %></td>
            </tr>
            <tr>
                    <th class="right-align">Gender: </th>
                    <td class="left-align"><%= insurance.sex %></td>
            </tr>
            <tr>
                    <th class="right-align">
                            Birth date:
                    </th>
                    <td class="left-align">
                            <%= insurance.birthdate %>
                    </td>
            </tr>
            <tr>
                    <th class="right-align">
                            Subscriber's Phone:
                    </th>
                    <td class="left-align">
                             <%= insurance.subscribers_phone %>
                    </td>
            </tr>
            <tr>
                    <th class="right-align">Subscriber Employer: </th>
                    <td class="left-align"><%= insurance.subscribers_employer %></td>
            </tr>
        </table>
	</fieldset>

