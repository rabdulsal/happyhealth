                    <th class="right-align">
                            Zip:
                    </th>
                    <td class="left-align">
                            <%= insurance.zipcode %>
                    </td>
            </tr>
            <tr>
                    <th class="right-align">
                            Group #:
                    </th>
                    <td class="left-align">
                             <%= insurance.group_number %>
                    </td>
            </tr>
            <tr>
                    <th class="right-align">Group Name: </th>
                    <td class="left-align"><%= insurance.group_name %></td>
            </tr>
            <tr>
                    <th class="right-align">Policy #: </th>
                    <td class="left-align"><%= insurance.policy_number %></td>
            </tr>
            <tr>
                    <th class="right-align">Subscriber's Last Name: </th>
