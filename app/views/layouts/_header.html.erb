<div id="header">
	<header id="header-content">
	     <ul>
	        <li id="site-name">
	        	<a href="/">HappyHealth</a>
	        </li>
        	<% if user_signed_in? %>
				<li class="nav-links">
					<%=link_to "Sign out", destroy_user_session_path, method: "delete" %>
				</li>
			<% else %>
				<li class="nav-links">
					<%=link_to "Sign In", new_user_session_path %>
				</li>
			<% end %>
				<li class="nav-links fluid-hide">
					<%=link_to "About Us", about_path%>
				</li>				
			<% if user_signed_in? %>	
				<li class="nav-links">
					<%=link_to "Dashboard", root_path %>
				</li>
				<li class="nav-links greeting">
					Hello <%= current_user.to_s %>
				</li>
			<% end %>				
	     </ul>   
	</header>
</div>