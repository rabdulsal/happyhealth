<h1>Listing visions</h1>

<table>
  <tr>
    <th>Insurance</th>
    <th>Eff date</th>
    <th>Dent company</th>
    <th>Policy number</th>
    <th>Group number</th>
    <th>Group name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @visions.each do |vision| %>
  <tr>
    <td><%= vision.insurance_id %></td>
    <td><%= vision.eff_date %></td>
    <td><%= vision.dent_company %></td>
    <td><%= vision.policy_number %></td>
    <td><%= vision.group_number %></td>
    <td><%= vision.group_name %></td>
    <td><%= link_to 'Show', vision %></td>
    <td><%= link_to 'Edit', edit_vision_path(vision) %></td>
    <td><%= link_to 'Destroy', vision, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vision', new_vision_path %>
