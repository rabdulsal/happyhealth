/* ----------------- FOOTER CSS ------------------------ */

footer {
    width: 100%;
    background: -webkit-linear-gradient(top, #eee, #fff);
    background:    -moz-linear-gradient(top, #eee, #fff);
    background:     -ms-linear-gradient(top, #eee, #fff);
    background: #4d7ca8;
    overflow: hidden;
    margin-top: 20px;
    max-height: 130px;
}

.footer-content {
    padding: 20px 0;    
    height: 200px;
    width: 980px;
    margin: 0 auto; 
    font-size: 16px;   
}

.copywrite, footer a {
    color: #000;
    text-decoration: none;
}

footer a:hover {
    color: #00B6E4;
    color: #f25d07;
}

.footer-links, .copywrite, footer a {
    float: right;
    list-style: none;
    margin-left: 5px;
    color: #888;
    color: #fff;  
}

.copywrite, #siteseal {
  float: left;
  margin-left: 0;
}

#siteseal img {
    height: 45px;
    width: 197px;
    padding-top: 10px;
}

footer ul li:last-child, footer a {
  margin-right: 0;
}


/* ----------------- RESET CSS ------------------------- */
.clearfix {
    *view: 1;
}

.clearfix:before, .clearfix:after {
    content: "";
    display: table;

}

.clearfix:after {
    clear:both;
}

/* --------------- RESPONSIVE DESIGN ------------------- */

@media all and (max-width: 480px) {
    article, aside, form, footer,
    .demoBackground, .left-panel,
    .dashAppointments, .dashBackground,
    #note_info, .widget, .description,
    .splash-page, .signin, .signup, .fluid-form, p a.pdfButton, 
    .right, .appointment-table, .appt-column, #appt.appt-button {
        float: none;
        
        width: 100%;
        /*margin: 0 2.0833333333333333%;*/
    }

    .noteBackground {        
        width: 91%;       
    }  

    .appointment-table {
        margin: 0;
    }

    .appointment-table li {
        margin-bottom: 10px;
    }

    .appt-column {
        height: 100%;
    }

    .appt-column p {
        padding: 10px;
    }

    textarea#note_info, .appt-column, .appt-field, .widget, .care-team-box li, .dashAppointments {
        width: 98%;
    }

    footer {
        padding: 0 2.0833333333333333%;
        margin-top: 0;
    }

    .description {
        height: 100%;
        margin-bottom: 10px;
    }
    .fluid-hide {
        display: none !important;
    }
    .container_12, #header-content, .widget-list {
        width: 95%;    
    }
    .container_12 {
        background: #fff !important;  
        margin-bottom: 0;
    }
    .signin, .signup {
        background: #f8f8f8;
        padding: 0 15px 20px 15px;
        margin-top: 20px;
    }
    .signin h2, .signup h2, article, .right-panel {
        margin-top: 10px;
    }

    .full_width, input.createButton {
        width: 93.9393939393939393%;
        margin: 0 2.08333%;
    }

    .action-button, .note-field, .note-button, .createButton {
        width: 100%;
    }

    .action-button, .note-field, .note-button {
        width: 100%;
    }

    p.signup-title {
        margin: 40px 5px;
        font-size: 20px;
    }
    #splash-title {
        font-size: 30px;
    }
    #subtitle {
        font-size: 23px;
    }
    #splash-text {
        font-size: 18px;
    }
