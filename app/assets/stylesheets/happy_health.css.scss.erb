/* --------------------- SHOW PAGES --------------------------- */

.show-table, .show-table-horizontal, .show-table-short {
    font-size: 14px; 
    border-collapse: collapse;   
}

.show-table th, .show-table-horizontal th, .show-table-short th {
    text-transform: uppercase;
    color: #aaa;
    color: #4d7ca8;
    font-size: 90%;    
    font-weight: bold;
    line-height: 190%;
    vertical-align: top;    
}

.show-table th, .show-table-short th {
    text-align: right;
    min-width: 180px;
}

.show-table-short th {
    max-width: 100px;
    padding-right: 10px;
}

.show-table-short th, .show-table-short td {
    
}

.show-table td, .show-table-horizontal td, .show-table-short td {    
    vertical-align: top;
    color: #273e54;
}

.show-table td {
    text-align: left;
    padding-left: 10px;
}

.show-table-horizontal td, .show-table-horizontal th {
    padding: 0 10px;    
}

.first-name, .last-name {
    width: 225px;
